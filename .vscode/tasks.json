{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-minimal",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/minimal/minimal.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "publish-minimal",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/minimal/minimal.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch-minimal",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "--project",
        "${workspaceFolder}/minimal/minimal.csproj"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-mvc",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/mvc/mvc.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "publish-mvc",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/mvc/mvc.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch-mvc",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "--project",
        "${workspaceFolder}/mvc/mvc.csproj"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
